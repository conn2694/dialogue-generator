# name of the file
var song = \"None\"

func _ready():
	pass # Replace with function body.


# for when we want to update the value when we change markers/pages
func updateControls(song):
	# Find the corresponding value in the node
	for i in range(get_node(\"Songs\").get_item_count()):
		if (song == get_node(\"Songs\").get_item_text(i)):
			get_node(\"Songs\").select(i)
			self.song = song



func _on_Songs_item_selected(index):
	song = get_node(\"Songs\").get_item_text(index)
	get_node(\"..\").updateControls()
"

[sub_resource type="GDScript" id=4]
script/source = "extends OptionButton



# Called when the node enters the scene tree for the first time.
func _init():
	self.add_item(\"N/A\")
	self.add_item(\"None\")
	dir_contents(loadPath())


# find the path that we set in portraitpath.txt to figure out where characters are
func loadPath():
	return GLOBALS.MUSIC_PATH
	
# Read in all the characters found in the directory
func dir_contents(path):
	var dir = Directory.new()
	if dir.open(path) == OK:
		dir.list_dir_begin()
		var file_name = dir.get_next()
		while file_name != \"\":
			# No subdirectories, and only png files
			if !dir.current_is_dir() && file_name.ends_with(\".ogg\"):
				# strip the .wav from files
				self.add_item(file_name.rstrip(\".ogg\"))
			file_name = dir.get_next()
	else:
		print(\"No Music directory found\")
"

